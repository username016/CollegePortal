@model CollegePortal.Entities.Models.StudyRoomBookings

<h1>Update Study Room Booking</h1>

<form asp-controller="StudyRoom" asp-action="Edit" method="post">
    <div class="form-group">
        <label for="StartTime">Start Time</label>
        <input asp-for="startTime" class="form-control" type="datetime-local" />
    </div>
    <div class="form-group">
        <label for="EndTime">End Time</label>
        <input asp-for="endTime" class="form-control" type="datetime-local" />
    </div>
    <button type="submit" class="btn btn-success">Update</button>
</form>
